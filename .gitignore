   # ignore .so files, which are generated by Cython
   *.so
   # ignore .c files, which are also generally Cython-generated
   *.c
   # except predict_c_funcs.c, the only .c file that is maintained by hand.
   !predict_c_funcs.c
   # ignore .o files
   *.o
   # ignore anything in the temporary directory temp_files
   **/temp_files
   # ignore anything in the results directory
   mcmm_results/*
   # all .DS_Store files
   .DS_Store
   */.DS_Store
   *.pyc

